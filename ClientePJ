public class ClientePJ extends Cliente {
        private String CNPJ;
        
        public ClientePJ(String nome,String endereco,String email,String cnpj) {
        	super(nome,endereco,email);
        	this.CNPJ = CNPJ;
        	
        public String getID() {
        	return this.CNPJ;
        }
        }
}
